#include <iostream>
using namespace std;

int main() {
	setlocale(LC_ALL, "Russian");
	int num;
	cout << "Enter a Number Between 1 - 15: ";
	cin >> num;

	if (num == 0 || num < 0 || num > 15) {
		cout << "Exiting the Programm";
		return 0;
	}
	int i = 1;
	while (i <= num) {
		cout << "Cycle:" << i << endl;
		i++;
	}
}
